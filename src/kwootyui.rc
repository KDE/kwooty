<?xml version="1.0" encoding="UTF-8"?>
<gui name="Kwooty"
        version="3"
        xmlns="http://www.kde.org/standards/kxmlgui/1.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.kde.org/standards/kxmlgui/1.0
        http://www.kde.org/standards/kxmlgui/1.0/kxmlgui.xsd" >
<MenuBar>
    <Menu name="file">
	   <Action name="clear"/>
    </Menu>
    <Menu name="download" ><text>Download</text>
	   <Action name="start"/>
	   <Action name="pause"/>
	   <Separator/>
	   <Action name="remove"/>
	   <Separator/>
	   <Action name="moveUp"/>
	   <Action name="moveDown"/>
	   <Separator/>
	   <Action name="moveTop"/>   
	   <Action name="moveBottom"/>
	   <Separator/>
	   <Action name="shutdown"/>
    </Menu>   
</MenuBar>

<ToolBar name="mainToolBar"> <text>Main Toolbar</text>
	<Action name="clear"/>
	<Separator/>
	<Action name="start"/>
	<Action name="pause"/>
	<Separator/>
	<Action name="remove"/>
	<Separator/>
	<Action name="moveUp"/>
	<Action name="moveDown"/>
	<Separator/>
	<Action name="downloadFolder"/>
	<Separator/>
	<Action name="shutdown"/>
</ToolBar>
</gui>